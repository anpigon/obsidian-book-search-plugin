  "main": "main.js",
  "standard-version": {
    "t": ""
  },
  "keywords": [
    "book",
    "book note",
    "obsidian",
    "plugin"
  ],
  "author": "anpigon",
  "license": "MIT",
  "engines": {
    "node": ">= 16",
    "yarn": ">= 1.22",
    "npm": "please use yarn"
