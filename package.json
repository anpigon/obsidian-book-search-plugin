    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "obsidian": "latest",
    "standard-version": "^9.3.2",
    "ts-jest": "^28.0.7",
